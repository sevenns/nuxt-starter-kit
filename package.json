{
  "name": "nuxt-starter-kit",
  "version": "1.0.0",
  "description": "Nuxt.js starter kit with pug, sass, svg icons, sass and es linters.",
  "author": "alexander sevenns",
  "private": true,
  "scripts": {
    "dev": "PORT=8080 nuxt",
    "build": "nuxt build",
    "start": "PORT=8080 nuxt start",
    "generate": "nuxt generate",
    "icons": "vsvg -s ./assets/icons/src -t ./assets/icons/dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "nuxt": "^1.0.0-rc11",
    "vue-svgicon": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-eslint": "^7.2.3",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "sanitize.css": "^5.0.0",
    "sass-loader": "^6.0.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
